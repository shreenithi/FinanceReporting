@model String

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Search</title>

    <link href="~/css/bootstrap.css" rel="stylesheet">
    <link href="~/css/style.css" rel="stylesheet">
    <link href="~/css/MenuFlyoutCss.css" rel="stylesheet">
    <link href="~/css/table.css" rel="stylesheet">

</head>

<body>
    <div class="container" style="padding-left: 0px; padding-right:0px;">
        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom">
                </div>
            </div>
            <div class="row">
                <div class="span12">
                    <a href="#" class="logo"> <span class="lite"><img alt="" src="img/t15.png" class="headerImg"></span></a>

                    <div class="top-nav notification-row">

                        <div style="padding: 15px; float:right;">
                            <b class="caret"></b>
                            <a href="/Login/Logout" style="color: white">Logout</a>
                        </div>

                    </div>
                </div>
            </div>
            <!--logo end-->

        </header>

        <div class="row">
            <div class="col-md-12">
                <div class="SecondHeader">
                    Collections Home
                    <div style="float: right; font-size: 16px;">
                        Welcome, @Session["name"].ToString()
                    </div>

                </div>
            </div>
        </div>

        <div class="row">


            <div class="col-md-12">
                <div id='content'>
                    <div id='SearchWrapper'>
                        <form>
                            <div class="col-md-4">
                                <input type="button" id="collRefresh" value="Refresh" class="button" style="width: 100%; margin:10px; margin-left:0px;" onclick="Call()" />
                            </div>
                            <div class="col-md-4">
                                <input type="button" id="collSendOM" value="Send to OM" class="button" style="width: 100%; margin:10px; margin-left:0px;" />
                            </div>
                            <div class="col-md-4">
                                <input type="button" id="collUpdateDel" value="Update Delinquent Customers" class="button" style="width: 100%; margin:10px; margin-left:0px;" />
                            </div>
                            <!-- <center>
                              <input type="button" id="login-submit" value="Search" class="button" />
                            </center> -->
                        </form>
                    </div>
                </div>

                <div id='content' style="top:200px">
                    Table here
                    <table class="TFtable">
                        <tr><th>Text</th><th>Text</th><th>Text</th></tr>
                        <tr><td>Text</td><td>Text</td><td>Text</td></tr>
                        <tr><td>Text</td><td>Text</td><td>Text</td></tr>
                        <tr><td>Text</td><td>Text</td><td>Text</td></tr>
                    </table>
                </div>

            </div>
        </div>
    </div>

</body>
</html>